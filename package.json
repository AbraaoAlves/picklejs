{
  "name": "picklejs",
  "version": "0.72.0",
  "private": true,
  "main": "buildLib",
  "dependencies": {
    "cypress-cucumber-preprocessor": "^1.2.2",
    "cypress-image-snapshot": "^2.0.1",
    "hex-rgb": "^3.0.0"
  },
  "scripts": {
    "start": "cd website && yarn start",
    "build": "babel lib/  --out-dir ./buildLib --source-maps && yarn version --new-version minor",
    "watch": "babel --watch lib/  --out-dir ./buildLib --source-maps",
    "buildSite": "cd website && yarn build",
    "prepush": "yarn build && yarn buildSite"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7"
  }
}
