{
  "name": "@tolicodes/picklejs",
  "version": "0.73.0",
  "main": "buildLib",
  "dependencies": {
    "cypress-cucumber-preprocessor": "^1.2.2",
    "cypress-image-snapshot": "^2.0.1",
    "hex-rgb": "^3.0.0"
  },
  "scripts": {
    "start": "cd website && yarn start",
    "build": "babel lib/  --out-dir ./buildLib --source-maps && yarn version --new-version minor",
    "watch": "babel --watch lib/  --out-dir ./buildLib --source-maps",
    "buildSite": "cd website && yarn build",
    "prepush": "yarn build && yarn buildSite",
    "cypress:open": "cd website && cypress open",
    "cypress:run": "cd website && yarn run cypress run --record --key <YOUR_KEY>",
    "cypress:ci": "cd website && yarn run cypress:run --parallel"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7"
  }
}
